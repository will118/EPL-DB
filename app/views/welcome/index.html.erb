<div class="jumbotron">
  <% if user_signed_in? %>
    <h1>Welcome back!</h1>
    <p class="lead">
      You have signed in!
    </p>
    <%= link_to "Sign Out", destroy_user_session_path, :method => :delete, :class => 'btn btn-large btn-danger' %>
  <% else %>
    <h1>Welcome, new user!</h1>
    <p class="lead">
      You must sign in or sign up. Github OAuth available.
    </p>
    <div class="row-fluid">
      
        <%= link_to "Sign In", new_user_session_path, :class => "btn btn-success" %>

      
        <%= link_to "Sign Up", new_user_registration_path, :class => "btn btn-info" %>
      
    </div>
  <% end %>
</div>